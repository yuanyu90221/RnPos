{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "tsc": "tsc",
    "clean": "rimraf dist",
    "lint": "tslint src/**/*.ts",
    "build": "npm run clean && npm run tsc --",
    "watch": "npm run build -- -w",
    "start:ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "start:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
    "release": "npm run build && react-native bundle --entry-file='dist/index.ios.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "fastlane": "npm run release && cd ios && fastlane beta"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "apollo-link-http": "^1.5.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "native-base": "^2.4.5",
    "react": "16.3.1",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.1.4",
    "react-native": "0.70.3",
    "react-native-gifted-chat": "^0.4.3",
    "react-navigation": "^2.1.0",
    "react-powerplug": "^1.0.0-alpha.7",
    "sendbird": "^3.0.63"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.16",
    "@types/react-navigation": "^2.0.2",
    "babel-jest": "23.0.0",
    "babel-preset-react-native": "4.0.0",
    "concurrently": "^3.5.1",
    "jest": "29.2.1",
    "react-test-renderer": "16.3.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
