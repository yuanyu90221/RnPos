{
  "name": "rnpos",
  "version": "1.0.0",
  "description": "react native pos system ",
  "main": "index.js",
  "scripts": {
    "install": "cd client && npm i && cd ../server && npm i",
    "start:ios":
      "cd client && npm i &&  npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "start:android":
      "cd client &&  npm i &&  npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
    "precommit": "npm run pretty-quick ",
    "pretty-quick": "pretty-quick --staged",
    "server": "cd server && npm i && nodemon index.js --ignore tests",
    "client": "cd client && npm run start:ios"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polo13999/RnPos.git"
  },
  "keywords": [
    "reactnative",
    "storybook",
    "ble",
    "jest",
    "typescript",
    "ci/cd",
    "realm",
    "apollographql",
    "mongodb"
  ],
  "author": "polo <polo13999@gmai.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/polo13999/RnPos/issues"
  },
  "homepage": "https://github.com/polo13999/RnPos#readme",
  "dependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "nodemon": "^1.17.5",
    "prettier": "^1.12.1",
    "concurrently": "^3.5.1"
    "pretty-quick": "^1.6.0",
    "react-native-gifted-chat": "^0.4.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
